{% do lms_env_config.update(EDXAPP_COMMON_PLATFORM_ENV) %}
{% do lms_env_config.update(EDXAPP_COMMON_LMS_ENV) %}
{% do lms_env_config.update(EDXAPP_PLATFORM_ENV_EXTRA) %}
{% do lms_env_config.update(EDXAPP_LMS_ENV_EXTRA) %}
{% for key, value in lms_env_config.iteritems() %}
  {% if value == 'None' %}
    {% do lms_env_config.update({key: None }) %} 
  {% endif %}
{% endfor %}
